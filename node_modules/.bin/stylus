#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/valerian/Mista-Val/Marion/mc-diamondz/node_modules/stylus/bin/node_modules:/Users/valerian/Mista-Val/Marion/mc-diamondz/node_modules/stylus/node_modules:/Users/valerian/Mista-Val/Marion/mc-diamondz/node_modules:/Users/valerian/Mista-Val/Marion/node_modules:/Users/valerian/Mista-Val/node_modules:/Users/valerian/node_modules:/Users/node_modules:/node_modules:/Users/valerian/Mista-Val/Marion/mc-diamondz/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/valerian/Mista-Val/Marion/mc-diamondz/node_modules/stylus/bin/node_modules:/Users/valerian/Mista-Val/Marion/mc-diamondz/node_modules/stylus/node_modules:/Users/valerian/Mista-Val/Marion/mc-diamondz/node_modules:/Users/valerian/Mista-Val/Marion/node_modules:/Users/valerian/Mista-Val/node_modules:/Users/valerian/node_modules:/Users/node_modules:/node_modules:/Users/valerian/Mista-Val/Marion/mc-diamondz/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../stylus/bin/stylus" "$@"
else
  exec node  "$basedir/../stylus/bin/stylus" "$@"
fi
